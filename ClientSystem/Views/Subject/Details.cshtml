@model ClientSystem.Models.Subject

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Details</h2>

<br />
<br />
<br />


<section class="content" style="text-align:center;margin-left:400px">
    <div class="row">

        <div class="col-md-6">

            <div class="box box-solid box-primary ">
                <div class="box-header with-border">
                    <h3 class="box-title">Subject  </h3>
                </div>
                <div class="box-body">
                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayName("Subject Name")
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.SubjectName)
                        </dd>

                        <dt>
                            @Html.DisplayName("Department Name")
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Department.DepartmentName)
                        </dd>

                    </dl>
                </div>
            </div>
        </div>
    </div>
</section>
